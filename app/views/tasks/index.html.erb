<p id="notice"><%= notice %></p>
<div class="videoVolumeBtn">
  <button class="unmotivated" onclick="toggleVideoVolume();">Motivation Disabled</button>
</div>
<video muted autoplay loop id="bgvid">
  <source src="<%= asset_path("justdoit.webm") %>" type="video/webm">
  <source src="<%= asset_path("justdoit.mp4") %>" type="video/mp4">
</video>
<h1>All Tasks</h1>
  <ul class="flex-boxes tasklist">
    <%= render @tasks %>
  </ul>
<br>

<%= link_to 'New Task', class: "new-task" %>
