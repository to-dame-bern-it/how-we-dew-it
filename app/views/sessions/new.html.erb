<p id="notice"><%= notice %></p>
<p id="alert"><%= alert %></p>

<div class="container">

<h1>Login</h1>


<%=form_tag(login_path) do%>
  <p><%=label_tag :email%></p>
  <p><%=email_field_tag :email%></p>

  <p><%=label_tag :password%></p>
  <p><%=password_field_tag :password%></p>

  <p><%= submit_tag "Submit"%></p>
<% end %>

  <%= link_to 'Sign Up', new_user_path %>
</div>
